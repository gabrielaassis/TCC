<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Aplicativo.TelaAceitarPartida">

    <Spinner
        android:id="@+id/spinnerAceitar"
        android:layout_width="344dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.106" />

    <TextView
        android:id="@+id/textView12312"
        android:layout_width="291dp"
        android:layout_height="wrap_content"
        android:text="txt view"
        android:textSize="40dp"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="35dp"
        app:layout_constraintTop_toBottomOf="@id/spinnerAceitar"
        tools:layout_editor_absoluteY="110dp" />

    <Button
        android:id="@+id/buttonteste"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="136dp"
        app:layout_constraintTop_toBottomOf="@id/textView12312"
        tools:layout_editor_absoluteY="215dp" />
</android.support.constraint.ConstraintLayout>